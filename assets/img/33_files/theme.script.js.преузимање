    /* Every time the window is scrolled ... */
    $(window).scroll( function(){

        /* Check the location of each desired element */
        $('.fade-in').each( function(i){

            var bottom_of_object = $(this).offset().top + $(this).outerHeight();
            var bottom_of_window = $(window).scrollTop() + $(window).height();

            /* If the object is completely visible in the window, fade it it */
            if( bottom_of_window > bottom_of_object ){

                $(this).animate({'opacity':'1'},400);

            }

        });


    });

    /* Every time the window is scrolled ... */
    $(window).scroll( function(){
        /* Check the location of each desired element */
        $('.left').each( function(i){

            var bottom_of_object = $(this).offset().top + $(this).outerHeight();
            var bottom_of_window = $(window).scrollTop() + $(window).height();

            /* If the object is completely visible in the window, fade it it */
            if( bottom_of_window > bottom_of_object ){

                if(app.width() < 770) {
                    $(this).animate({left: '0px'},0);
                }else{
                    $(this).animate({left: '70px'},500);
                }

            }

        });
    });

    /* Every time the window is scrolled ... */
    $(window).scroll( function(){
        /* Check the location of each desired element */
        $('.right').each( function(i){

            var bottom_of_object = $(this).offset().top + $(this).outerHeight();
            var bottom_of_window = $(window).scrollTop() + $(window).height();

            /* If the object is completely visible in the window, fade it it */
            if( bottom_of_window > bottom_of_object ){

                if(app.width() < 770) {
                    $(this).animate({right: '0px'},0);
                }else{
                    $(this).animate({right: '70px'},400);
                }

            }

        });
    });

    $(window).scroll(function(){
        if ($(this).scrollTop() > 10) {
            $('.about-image-item').addClass('aaa');
        } else {
            $('.about-image-item').removeClass('aaa');
        }
    });

    if(app.width() < 768) {
        $('.img-responsive').removeClass('left');
        $('.img-responsive').removeClass('right');
    }

    $(".country-btn").click(function(){
        $value = $("#myselect").val();

        if ($value == 1){
            window.location = "https://www.mona.rs/SRB_rs";
        }else {
            window.location = "https://www.mona.rs/GLO_en";
        }
    });

    $('#reg_loyaltyRequest').change(function() {
        if(this.checked) {
            $('#reg_sendNewsletter,#reg_callMeOnPhone,#reg_sendSMS,#reg_sendUserAdditionalData').each(function(){
                if(!this.checked){
                    $(this).click();
                }
            });
        }
    });

    $('[rel=tooltip]').tooltip('disable');


});
