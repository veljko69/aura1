        digits: getSTRINGParsly('Unesite validan broj sa decimalama.'),
        alphanum: getSTRINGParsly('Unesite validan alfanumerički broj.')
    },
    notblank: getSTRINGParsly('Ovo polje ne sme biti prazno.'),
    required: getSTRINGParsly('Ovo polje je obavezno.'),
    pattern: getSTRINGParsly('Ovo polje nije validno.'),
    remote: "",
    min: getSTRINGParsly('Ovo polje nije veće ili jednako') + "%s.",
    max: getSTRINGParsly('Ovo polje nije manje ili jednako') + "%s.",
    range: getSTRINGParsly('Ovo polje nije između') + "%s" + getSTRINGParsly('i') + "%s.",
    minlength: getSTRINGParsly('Ovo polje sadrži manje od') + "%s." + getSTRINGParsly('karaktera.'),
    maxlength: getSTRINGParsly('Ovo polje sadrži više od') + "%s." + getSTRINGParsly('karaktera.'),
    length: getSTRINGParsly('Dužina ovog polja mora biti između ') + "%s" + getSTRINGParsly('i') + "%s." + getSTRINGParsly('karaktera.'),
    mincheck: getSTRINGParsly('Izaberite najmanje ') + "%s" + getSTRINGParsly('opcije.'),
    maxcheck: getSTRINGParsly('Izaberite ') + "%s" + getSTRINGParsly(' opcije ili manje.'),
    check: getSTRINGParsly('Izaberite između ') + "%s" + getSTRINGParsly('i') + "%s." + getSTRINGParsly(' opcije.'),
    equalto: getSTRINGParsly('Ovo polje nije jednako.'),
    equaltovalue: getSTRINGParsly('Ovo polje nije jednako vrednosti') + "%s."
};

